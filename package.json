{
  "name": "reactizer",
  "version": "0.10.1",
  "description": "The ultimate react boilerplate!",
  "main": "dist/server.js",
  "scripts": {
    "start": "rimraf dist && gulp && run-p devserver webpack:dev",
    "devserver": "yarn babel && run-p run:dev babel:dev",
    "run": "node dist/server",
    "run:dev": "nodemon dist/server",
    "render": "node dist/server/render",
    "bundle": "rimraf data dist && gulp && npm run babel && npm run webpack && npm run render",
    "babel": "babel src -d dist --ignore '**/__tests__/*'",
    "babel:dev": "babel src -d dist --ignore '**/__tests__/*' --watch --source-maps inline",
    "webpack": "webpack --config etc/webpack.build.js --mode production",
    "webpack:dev": "webpack-dev-server --config etc/webpack.dev.js --mode development --inline --hot",
    "test": "jest",
    "lint": "eslint './**/*.{js,jsx}'",
    "madge": "madge -c src/server/index.js src/client/index.js",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o dist-storybook",
    "example": "rimraf docs/app docs/storybook && run-p example:app example:storybook",
    "example:app": "yarn bundle && copyfiles -u 2 'dist/static/**/*' docs/app && copyfiles -u 5 docs/app/pages/main/en/index.html docs/app",
    "example:storybook": "build-storybook -o docs/storybook"
  },
  "bin": {
    "reactizer": "./bin/reactizer.js"
  },
  "keywords": [
    "react",
    "webpack",
    "boilerplate"
  ],
  "author": "oreqizer",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^7.0.1",
    "koa": "^2.7.0",
    "koa-logger": "^3.2.0",
    "koa-static": "^5.0.0",
    "node-polyglot": "^2.3.0",
    "ramda": "^0.26.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-hot-loader": "^4.6.5",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "4.1.12",
    "@storybook/addon-knobs": "^4.1.12",
    "@storybook/addon-links": "4.1.12",
    "@storybook/addons": "4.1.12",
    "@storybook/react": "4.1.12",
    "assets-webpack-plugin": "^3.9.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "8.0.5",
    "babel-plugin-id": "^1.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-ramda": "^2.0.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-airbnb": "^3.2.0",
    "copyfiles": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.14.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.93.0",
    "gulp": "^4.0.0",
    "jest": "^24.1.0",
    "jest-styled-components": "^6.3.1",
    "madge": "^3.4.4",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "raf": "^3.4.1",
    "rimraf": "^2.6.3",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  }
}
