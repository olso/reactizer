{
  "name": "reactizer",
  "version": "0.11.0",
  "description": "The ultimate React boilerplate!",
  "main": "dist/server.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "bundle": "printenv && rimraf dist && gulp && yarn build",
    "syncgql": "run-s schema generate relay",
    "relay": "relay-compiler --src src --schema ./schema.graphql --extensions ts tsx --artifactDirectory src/__generated__ --language typescript",
    "relay:validate": "yarn relay --validate",
    "schema": "graphql get-schema",
    "generate": "graphql-codegen",
    "keys": "node etc/bin/keys",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "report": "env NODE_ENV=production yarn webpack --json > stats.json",
    "types": "tsc",
    "audit": "yarn audit --prod",
    "ci": "run-p test:coverage lint types madge",
    "lint": "eslint './**/*.{js,ts,tsx,jsx}'",
    "madge": "madge -c src/index.ts",
    "storybook": "start-storybook -p 6006 -s static",
    "storybook:build": "build-storybook -o dist/static/storybook -s static",
    "heroku-postbuild": "yarn bundle"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "always",
        "sentence-case"
      ]
    }
  },
  "resolutions": {
